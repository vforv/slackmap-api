{
  "name": "@slackmap/api",
  "version": "0.0.1",
  "description": "SlackMap API Server",
  "scripts": {
    "start": "NODE_ENV=dev ts-node-dev -r tsconfig-paths/register --fast --respawn ./src/server.mock.ts",
    "start2": "nodemon --exec \"ts-node -r tsconfig-paths/register\" ./src/server.ts",
    "build": "tsc",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "lint:fix": "tslint -c tslint.json src/**/*.ts --fix",
    "test": "NODE_ENV=testrun mocha -r ts-node/register -r tsconfig-paths/register './src/**/*.spec.ts'",
    "e2e": "NODE_ENV=testrun mocha -r ts-node/register -r tsconfig-paths/register './e2e/**/*.e2e-spec.ts'",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "@nestjs/common": "^4.5.6",
    "@nestjs/core": "^4.5.7",
    "@nestjs/swagger": "^1.2.0",
    "@nestjs/testing": "^4.5.5",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.11",
    "lowdb": "^1.0.0",
    "method-override": "^2.3.10",
    "nodemon": "^1.14.9",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1",
    "randomstring": "^1.1.5",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.5.6",
    "speakingurl": "^14.0.1",
    "statuses": "^1.4.0",
    "ts-node": "^3.3.0",
    "uuid": "^3.1.0",
    "snyk": "^1.67.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/chai": "^4.1.0",
    "@types/chai-http": "^3.0.3",
    "@types/express": "^4.11.0",
    "@types/geojson": "^7946.0.0",
    "@types/jsonwebtoken": "^7.2.5",
    "@types/lodash": "^4.14.92",
    "@types/lowdb": "^0.15.0",
    "@types/method-override": "0.0.31",
    "@types/mocha": "^2.2.46",
    "@types/node": "^8.5.8",
    "@types/passport": "^0.4.2",
    "@types/passport-jwt": "^3.0.0",
    "@types/randomstring": "^1.1.6",
    "@types/speakingurl": "^13.0.2",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "chai-like": "^1.0.1",
    "mocha": "^4.1.0",
    "ts-node-dev": "^1.0.0-pre.7",
    "tsconfig-paths": "^2.7.3",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.6.0",
    "typescript": "^2.6.2"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SlackMap/slackmap-api.git"
  },
  "keywords": [
    "slackline",
    "map",
    "typescript",
    "nodejs",
    "nestjs",
    "swagger",
    "express"
  ],
  "author": "@myflowpl",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/SlackMap/slackmap-api/issues"
  },
  "homepage": "https://github.com/SlackMap/slackmap-api#readme",
  "snyk": true
}
