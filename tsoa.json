{
  "swagger": {
      "outputDirectory": "./src/rest/docs",
      "entryFile": "./src/rest/app.ts",
      "host": "localhost:3000",
      "basePath": "/api/v2",
      "securityDefinitions": {
        "api_token": {
            "type": "apiKey",
            "name": "api_token",
            "in": "query",
            "description": ""
        }
    }
  },
  "routes": {
    "basePath": "/api/v2",
    "entryFile": "./src/rest/app.ts",
    "routesDir": "./src/rest",
    "middleware": "express",
    "middlewareTemplate": "./src/rest/routes-template.ts",
    "iocModule": "./src/ioc.ts",
    "authenticationModule": "./src/rest/auth.ts"
  }
}